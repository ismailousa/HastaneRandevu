@model IList<HastaneRandevu.Areas.Admin.ViewModels.CinsiyetRadioBox>
<div class="panel panel-default">
    <div class="panel-heading">Cinsiyet</div>
    <div class="panel-body">
        <div class="list-group">
            @for (int i = 0; i < Model.Count; i++)
            {
                <div class="list-group-item" style="margin-left:70px">
                    @Html.Hidden("Cinsiyetler[" + i + "].Id", Model[i].Id)
                    <label for="Cinsiyetler_@(i)__IsChecked">
                        @Html.CheckBox(@*"cinsiyet",*@"Cinsiyetler[" + i + "].IsChecked", Model[i].IsChecked)
                        @Model[i].Name
                    </label>
                </div>
            }
        </div>
    </div>
</div>
